#include<stdio.h>
#include<conio.h>
int main()
{
	int low,a[10],n,i;
	printf("Enter array size:");
	scanf("%d",&n);
	printf("\nEnter array elments:");
	for(i=0;i<n;i++)
	 scanf("%d",&a[i]);
	low=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]<low)
		 low=a[i];
	}
	printf("\nLeast number=%d",low);
	getch();
	return 0;
}
